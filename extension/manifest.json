{
  "manifest_version": 3,
  "name": "Redirect Blocker",
  "version": "4.0.0",
  "background": {
    "service_worker": "./dist/background.js"
  },
  "description": "Redirect Blocker saves you from facing potential malicious/annoying redirects.",
  "icons": {
    "16": "./img/Icon16.png",
    "32": "./img/Icon32.png",
    "128": "./img/Icon128.png"
  },
  "permissions": ["tabs", "storage"]
}
